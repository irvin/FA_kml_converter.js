<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<gpx xmlns="http://www.topografix.com/GPX/1/1" version="1.1" creator="FlightAwareRouteConverter">
    <metadata>
        <name>{{name}}</name>
    </metadata>
    <rte>
        <name>{{name}}'s Route</name>
        {{#each coor}}
        <rtept lon="{{this.lon}}" lat="{{this.lat}}">
            <time>{{this.time}}</time>
            <geoidheight>{{this.height}}</geoidheight>
            <name>Waypoint {{@index}}</name>
        </rtept>
        {{/each}}
    </rte>
</gpx>